{
  "openapi": "3.0.1",
  "info": {
    "title": "All The Flavors",
    "description": "This is the second version of the ATF API. Find out more at the [official site](https://alltheflavors.com).",
    "termsOfService": "https://app.termly.io/document/terms-of-use-for-website/ea0409b4-1130-468a-b405-e44331c15846",
    "contact": {
      "url": "https://alltheflavors.com"
    },
    "version": "2.0.0"
  },
  "servers": [
    {
      "url": "https://alltheflavors.com/api/v2"
    },
    {
      "url": "http://alltheflavors.com/api/v2"
    }
  ],
  "tags": [
    {
      "name": "recipe",
      "description": "A recipe of one or more flavorings"
    },
    {
      "name": "flavor",
      "description": "A flavoring"
    },
    {
      "name": "tag",
      "description": "A tag describing another entity"
    },
    {
      "name": "user",
      "description": "A user"
    }
  ],
  "paths": {
    "/recipes": {
      "get": {
        "tags": [
          "recipe"
        ],
        "summary": "Get a list of recipes",
        "operationId": "getRecipes",
        "responses": {
          "200": {
            "description": "List of recipes",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RecipeResponse"
                }
              }
            }
          }
        }
      }
    },
    "/flavors": {
      "get": {
        "tags": [
          "flavor"
        ],
        "summary": "Get a list of flavors",
        "responses": {
          "200": {
            "description": "List of flavors",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/FlavorResponse"
                }
              }
            }
          }
        }
      }
    },
    "/tags": {
      "get": {
        "tags": [
          "tag"
        ],
        "summary": "Get a list of tags",
        "responses": {
          "200": {
            "description": "List of tags",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/TagResponse"
                }
              }
            }
          }
        }
      }
    },
    "/users": {
      "get": {
        "tags": [
          "user"
        ],
        "summary": "Get a list of users",
        "responses": {
          "200": {
            "description": "List of users",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Recipe": {
        "type": "object",
        "properties": {
          "mix_id": {
            "type": "integer",
            "format": "int64",
            "description": "Internal identifier",
            "example": 2088772058219
          },
          "mix_sid": {
            "type": "string",
            "description": "External base36 identifier",
            "example": "qnkgyb1n"
          },
          "name": {
            "type": "string"
          }
        },
        "xml": {
          "name": "Recipe"
        }
      },
      "Flavor": {
        "type": "object",
        "properties": {
          "mix_id": {
            "type": "integer",
            "format": "int64",
            "description": "Internal identifier",
            "example": 2088772058219
          },
          "mix_sid": {
            "type": "string",
            "description": "External base36 identifier",
            "example": "qnkgyb1n"
          },
          "username": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "userStatus": {
            "type": "integer",
            "description": "User Status",
            "format": "int32"
          }
        },
        "xml": {
          "name": "Flavor"
        }
      },
      "Tag": {
        "type": "object",
        "properties": {
          "mix_id": {
            "type": "integer",
            "format": "int64",
            "description": "Internal identifier",
            "example": 2088772058219
          },
          "mix_sid": {
            "type": "string",
            "description": "External base36 identifier",
            "example": "qnkgyb1n"
          },
          "name": {
            "type": "string"
          },
          "taggings_count": {
            "type": "integer",
            "format": "int32"
          }
        },
        "xml": {
          "name": "Tag"
        }
      },
      "User": {
        "required": [
          "id",
          "name"
        ],
        "type": "object",
        "properties": {
          "mix_id": {
            "type": "integer",
            "format": "int64",
            "description": "Internal identifier",
            "example": 2088772058219
          },
          "mix_sid": {
            "type": "string",
            "description": "External base36 identifier",
            "example": "qnkgyb1n"
          },
          "username": {
            "type": "string",
            "example": "someone"
          },
          "recent_recipes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Recipe"
            }
          },
          "featured_recipes": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Recipe"
            }
          },
          "recipes": {
            "type": "object",
            "description": "Recipe counts",
            "properties": {
              "total": {
                "type": "integer",
                "format": "int32"
              },
              "private": {
                "type": "integer",
                "format": "int32"
              },
              "public": {
                "type": "integer",
                "format": "int32"
              }
            }
          }
        },
        "xml": {
          "name": "User"
        }
      },
      "RecipeResponse": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Recipe"
        }
      },
      "FlavorResponse": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Flavor"
        }
      },
      "TagResponse": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/Tag"
        }
      },
      "UserResponse": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/User"
        }
      }
    }
  }
}